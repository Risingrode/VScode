{
    "manifest_version": 2,
    "name": "翻译插件",
    "version": "1.0",
    "description": "这是一个插件",
    "icons": {
        "128": "img/logo.png",
        "48": "img/logo.png",
        "16": "img/logo.png"
    },
    "browser_action": {
        "default_icon": "img/logo.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*/*"],
        "js": ["index.js"]
    }]



}